package carreradecaballos;

import java.util.Observable;
import java.util.Random;

public class Caballos extends Observable implements Runnable
{
	private Random rand;
	
	public Caballos() 
	{
		rand = new Random();
	}

	@Override
	public void run() 
	{
		while(true) 
		{
			try 
			{
				Thread.sleep(1000);
				
			} catch (InterruptedException e) {}
		}
		
	}
	
	

}
