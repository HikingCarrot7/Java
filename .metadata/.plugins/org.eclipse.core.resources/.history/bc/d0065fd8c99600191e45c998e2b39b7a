package empleados;

public class Test 
{
	
	private String[] nombres = {"Fernando", "Nicolás", "Carlos", "Luis"};

	public static void main(String[] args) 
	{
		new Test().main();

	}
	
	public void main() 
	{
		System.out.println("Java-is-cool");
		
		for(int i = 0; i < nombres.length; i++) 
		{
			if(i % 2 == 0) 
			{
				continue;
			}
			
			System.out.println(nombres[i]);
			
			System.out.println("Las esperanzas de las mañanas de las estancias de las maneras de las cuales se pueden llegar a estar");
		
		}
		
	}
	
}