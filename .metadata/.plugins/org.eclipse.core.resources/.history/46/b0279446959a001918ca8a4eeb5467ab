package serializable;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.ObjectInputStream;
import java.io.ObjectOutputStream;

public class PruebaAlumnos 
{
	private static Alumnos[] misAlumnos;
	
	public static void main(String[] args) 
	{
		misAlumnos = new Alumnos[2];
		
		misAlumnos[0] = new  Alumnos("Nicolás");
		misAlumnos[1] = new  Alumnos("Fernando");
		
		escribirAlumnos();
		
		leerAlumnos();
		
	}
	
	public static void escribirAlumnos() 
	{
		try 
		{
			ObjectOutputStream file = new ObjectOutputStream(new FileOutputStream("C:\\Archivos\\Prueba.txt"));
			
			file.writeObject(misAlumnos);
			
			file.close();
			
		}catch(IOException e) 
		{
			
		}
	}
	
	public static void leerAlumnos() 
	{
		try 
		{
			ObjectInputStream file = new ObjectInputStream(new FileInputStream("C:\\Archivos\\Prueba.txt"));
			
			
			
			file.close();
			
		} catch (FileNotFoundException e) 
		{

		} catch (IOException e) 
		{

		}
		
	}
	
}
