package practicadialogos;

import java.awt.GridLayout;

import javax.swing.JPanel;

public class Lamina extends JPanel
{
	private static final long serialVersionUID = 1L;
	
	private String[] primero = {"Mensaje", "Confirmar", "Opción", "Entrada"};
	
	private String[] segundo = {"ERROR_MESSAGE", "INFORMATION_MESSAGE", "WARNING_MENSSAGE", "QUESTION_MENSSAGE", "PLAIN_MENSSAGE"};
	
	private String[] tercero = {"Cadena", "Icono", "Componente", "Otros", "Object[]"};
	
	public Lamina() 
	{
		setLayout(new GridLayout(1,3));
		
		add(new LaminaSecciones("Tipo", primero));
		add(new LaminaSecciones("Tipo mensaje", segundo));
		add(new LaminaSecciones("Mensaje", tercero));
		
	}
	
}
