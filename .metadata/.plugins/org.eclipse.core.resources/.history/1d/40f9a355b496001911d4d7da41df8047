package empleados;

public class PruebaEmpleado 
{
	private int i = 50;

	public static void main(String[] args) 
	{
		Empleados[] misEmpleados = new Empleados[2];
		
		System.out.println(new PruebaEmpleado().cambiarI(200));
		
		System.out.println(new PruebaEmpleado().i);
		
		misEmpleados[0] = new Empleados("Nicolás" , 45634);
		misEmpleados[1] = new Empleados("Alexis", 54323);
		
		for(Empleados M: misEmpleados)
			System.out.println(M);

	}
	
	public int cambiarI(int nuevoValor) 
	{
		i = nuevoValor;
		
		return i;
	}

}
