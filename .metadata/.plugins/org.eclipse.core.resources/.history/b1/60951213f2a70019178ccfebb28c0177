package clientes;

import java.util.HashSet;
import java.util.Iterator;
import java.util.Set;

public class PruebaClientes 
{
	
	private static Set<Clientes> clientes;
	
	public static void main(String[] args) 
	{
		Clientes c1 = new Clientes("Nicolás", "01", 5000);
		Clientes c2 = new Clientes("Fernando", "02", 6000);
		Clientes c3 = new Clientes("Luis", "03", 7000);
		Clientes c4 = new Clientes("Carlos", "04", 8000);
		
		
		clientes = new HashSet<>();
		
		clientes.add(c1);
		
		for (Iterator<Clientes> iterator = clientes.iterator(); iterator.hasNext();) 
			System.out.println(iterator);
		
	}
	
	
	
	

}
